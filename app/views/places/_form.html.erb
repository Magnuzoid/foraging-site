<div class="overlay">
  <%= form_for @place, method: "create", id: "new_place", remote: true do |f| %>
    <div class="row">
      <%= f.label :title %><br />
      <%= f.text_field :title %>
    </div>
    <div class="row">
      <%= f.label :description %><br />
      <%= f.text_area :description %>
    </div>
    <div class="row">
      <%= f.text_field :lat, hidden: true %><br />
      <%= f.text_field :lng, hidden: true %>
    </div>
    <%= f.submit nil %>
    <a onClick="closeDialouge()" class="close">x</a>
  <% end %>
</div>
